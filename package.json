    "@types/lodash": "4.14.149",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "7.1.7",
    "@types/react-router": "5.1.5",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-logger": "3.0.7",
    "@types/webpack-env": "1.15.1",
